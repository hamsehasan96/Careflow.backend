{
  "name": "server",
  "version": "1.0.0",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sonar": "sonar-scanner",
    "migrate": "sequelize-cli db:migrate",
    "seed": "node src/utils/seedDatabase.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "postinstall": "if [ \"$NODE_ENV\" != \"production\" ]; then npm run setup-husky; fi",
    "setup-husky": "husky install",
    "build": "echo 'No build step required'",
    "clean": "rimraf node_modules package-lock.json",
    "clean:install": "npm run clean && npm install",
    "verify": "npm run lint && npm run test",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use Yarn instead of npm')\""
  },
  "engines": {
    "node": ">=18.x <23.0.0",
    "npm": ">=10.2.4"
  },
